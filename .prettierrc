{
    "arrowParens": "avoid",
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true,
    "jsxSingleQuote": true,
    "experimentalTernaries": true,
    "printWidth": 80,
    "tabWidth": 4,
    "plugins": [
        "prettier-plugin-tailwindcss",
        "prettier-plugin-classnames",
        "prettier-plugin-merge"
    ],
    "experimentalOptimization": true,
    "endingPosition": "absolute-with-indent"
}
